project ISO_3166_Java is
   for Languages use ("Java");
   for Create_Missing_Dirs use "True";
   for Source_Dirs use ("src/Java/**");
   for Object_Dir use "bin/Java";

   package Compiler is
      for Driver ("Java") use "javac";
   end Compiler;

   package Naming is
      for Implementation_Suffix ("Java") use ".java";
   end Naming;

end ISO_3166_Java;

